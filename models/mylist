const mongoose = require('mongoose');
const Schema = mongoose.Schema;

const mylistSchema = new Schema({
    gameId: {
      type: Number,
      required: true,
    },
    user: {
      type: Schema.Types.ObjectId,
      ref: 'User',
    },
  }, {
    timestamps: true
  });

module.exports = mongoose.model('Mylist', mylistSchema);